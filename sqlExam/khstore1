--취미 테이블 생성
CREATE TABLE hobbies (
MNO NUMBER PRIMARY KEY, 
--NUMBER는 소수점이하까지 허용, INT는 소수점이하가 포함되지 않는다는 뜻 (PRIMARY KEY에 INT도 가능)
HOBBY VARCHAR(100)
);

--멤버들의 취미 데이터 삽입
INSERT INTO HOBBIES (MNO, HOBBY)
 --VALUES (1, '책읽기'); (하나하나 직접 다 작성해줘야함)
